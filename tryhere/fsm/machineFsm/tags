AbstractState	MachineStates.hh	/^class AbstractState {$/;"	c
Machine	Machine.cc	/^Machine::Machine(int inStockQuantity) :$/;"	f	class:Machine
Machine	Machine.hh	/^class Machine {$/;"	c
Normal	MachineStates.hh	/^class Normal : public AbstractState {$/;"	c
SoldOut	MachineStates.hh	/^class SoldOut : public AbstractState {$/;"	c
getCurrentStock	Machine.cc	/^int Machine::getCurrentStock() {$/;"	f	class:Machine
mState	Machine.hh	/^   AbstractState* mState;$/;"	m	class:Machine
mStockQuantity	Machine.hh	/^   int mStockQuantity;$/;"	m	class:Machine
main	MachineFsm.cc	/^int main() {$/;"	f
refill	Machine.cc	/^void Machine::refill(int quantity) {$/;"	f	class:Machine
refill	MachineStates.cc	/^void Normal::refill(Machine& machine, int quantity) {$/;"	f	class:Normal
refill	MachineStates.cc	/^void SoldOut::refill(Machine& machine, int quantity) {$/;"	f	class:SoldOut
sell	Machine.cc	/^void Machine::sell(int quantity) {$/;"	f	class:Machine
sell	MachineStates.cc	/^void Normal::sell(Machine& machine, int quantity) {$/;"	f	class:Normal
sell	MachineStates.cc	/^void SoldOut::sell(Machine& machine, int quantity) {$/;"	f	class:SoldOut
setState	MachineStates.cc	/^void AbstractState::setState(Machine& machine, AbstractState* state) {$/;"	f	class:AbstractState
updateStock	MachineStates.cc	/^void AbstractState::updateStock(Machine& machine, int quantity) {$/;"	f	class:AbstractState
~AbstractState	MachineStates.cc	/^AbstractState::~AbstractState() {$/;"	f	class:AbstractState
~Machine	Machine.cc	/^Machine::~Machine() {$/;"	f	class:Machine
~Normal	MachineStates.cc	/^Normal::~Normal() {$/;"	f	class:Normal
~SoldOut	MachineStates.cc	/^SoldOut::~SoldOut() {$/;"	f	class:SoldOut
